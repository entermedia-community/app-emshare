{
	"model": "${model}",
	"messages": [
		{
			"role": "system",
			"content": #jesc($fieldparams.aicreationdescription)
		},
		{
			"role": "user",
			"content": "$fieldparams.aicreationcommand #foreach($field in $contextfields) #if($foreach.count>1),\n#end ${field.label}: ${field.text} #end"
		}
	],
	"stream": false,
	"think": false,
	"temperature": 0,
	"format": {
		"type": "object",
		"properties": {
			"categories": {
				"type": "object",
				"properties": {
					#foreach($table in $tables)
						#if($foreach.count>1)
						,
						#end
						"$table.sourcetype": {
							"type": "array",
							"description": "Names of $table.label",
							"items": {
								"type": "string",
								"additionalProperties": false
							}
						}
					#end
				}
			}
		},
		"required": ["categories"]
	}
}
