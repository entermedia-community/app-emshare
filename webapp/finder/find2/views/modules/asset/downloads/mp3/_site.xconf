<?xml version="1.0" encoding="UTF-8"?>
                                                                                                                                   
<page>
	<property name="assetrootfolder">/${applicationid}/views/modules/asset/downloads/mp3</property>
	<property name="sourcepathhasfilename">true</property>
	<generator name="convertArchiveDocument"/>
	<property name="bitrate">64</property>
	<property name="useoriginalmediawhenpossible">true</property>
	
	<permission name="view">
		<pagevalue name="canviewasset" equals="true"/>
	</permission>	
	
</page>


