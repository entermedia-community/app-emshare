<page>

	<path-action name="EntityModule.loadSelectedEntity"/>

	<path-action name="DataEditModule.search">
		<searchtype>${module}</searchtype>
		<property name="hitsname">hits</property>
		<property name="clearfilters">true</property>
		<property name="clearselection">${module}</property>
	</path-action>
	
	<path-action name="ProfileModule.savePreference"/>
		
	<inner-layout>/${applicationid}/views/modules/${module}/innerlayout.html</inner-layout>

	
</page>