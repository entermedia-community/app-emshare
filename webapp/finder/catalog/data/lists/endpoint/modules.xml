  <data id="moduleretrieve" name="Load Data " httpmethod="GET" url="/${applicationid}/services/settings/modules/data/testmodule" docsection="modules"/>  
  <data id="moduleupdate" caption="Update data with key/values in request" name="Update Data" httpmethod="PUT" url="/${applicationid}/services/settings/modules/data/testmodule" docsection="modules"> 
    <samplerequest><![CDATA[{  "name": "Test Module2"
   }]]></samplerequest> 
  </data>  
  <data id="moduledelete" caption="Delete Data" name="Delete Data" httpmethod="DELETE" url="/${applicationid}/services/settings/modules/data/testmodule" docsection="modules"/>

 </endpoints>
