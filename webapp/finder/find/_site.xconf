  <permission name="quicksearch">
  	<userprofile property="quicksearch" equals="true" />
  </permission>
  
  
  	<!-- Default ROLE BASED PERMISSIONS, can be customized -->
	<permission name="editasset">
		<or>
			<group name="administrators" />
			<userprofile property="editallassets" equals="true" />
			<action name="AssetControlModule.canEditAsset" />
		</or>
	</permission>
	
	<permission name="editcollection">
		<or>
			<group name="administrators" />
			<userprofile property="editcollection" equals="true" />
			<action name="ProjectModule.canEditCollection" />
		</or>
	</permission>
	
 <permission name="viewcategorynav">
     <userprofile property="viewcategorynav" equals="true" />
  </permission>

	
		
</page>
