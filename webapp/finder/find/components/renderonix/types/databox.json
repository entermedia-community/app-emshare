[
	{
    "type": "draw2d.shape.composite.Group",
    "id": "id_${node.id}",
    "x": ${node.offetX(250,0)},
 #set( $topy = ${node.rowoffet(50,20))   
    "y": $topy,
    "width": 200,
    "height": ${node.getDataTotalHeight(60)},
    "bgColor": "#60729e",
    "color": "#000000",
    "stroke": 1,
    "cssClass": "labelGroup",
    "radius": 4,
    "selectable": true,
    "draggable": false,
    "ports": [
      {
        "id": "portleft_${node.id}",
        "name": "portleft_${node.id}",
		    "type": "draw2d.HybridPort", 
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
        "width": 8,
        "height": 8,
        "selectable": false,
        "draggable": false,
        "bgColor": "#60729e",
        "color": "#4d5d80",
        "stroke": 1,
        "locatorAttr": {
          "x": 0,
          "y": 50
        }
      }
      #if( !$node.getChildren().isEmpty() )
      ,
      {
        "id": "portright_${node.id}",
        "name": "portright_${node.id}",
        "type": "draw2d.HybridPort", 
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
        "width": 8,
        "height": 8,
        "selectable": false,
        "draggable": false,
        "bgColor": "#60729e",
        "color": "#4d5d80",
        "stroke": 1,
        "locatorAttr": {
          "x": 100,
          "y": 50
         }
      }
      #end
    ]
  }
#foreach( $datanode in $node.getDataChilren() ) 
  , {
    "type": "draw2d.shape.basic.Label",
  	"id": "idtext_${datanode.id}",
    "x": ${datanode.offetX(250,5)},
#set( $index = $foreach.count - 1 )
#set( $topy = $topy + $index*60 )
    "y": $topy,
    "text": "$datanode.getTextTrim()", 
    "stroke": 0,
    "fontSize": 20,
    "fontColor": "#ffffff",
    "bgColor": "none",
    "selectable": false,
    "draggable": false,
    "cssClass": "titleLabel",
    "composite": "id_${node.id}"
  }
#end  
  , {
    "type": "draw2d.Connection", 
    "userData": {},
    "cssClass": "draw2d_Connection",
    "stroke": 2,
    "color": "#4caf50", 
    "outlineColor": "#ffffff", 
    "router": "draw2d.layout.connection.InteractiveManhattanConnectionRouter",
    "radius": 40,
    "source": {
      "node": "id_${node.sourceId}",
      "port": "portright_${node.sourceId}"
    },
    "target": {
      "node": "id_${node.id}",
      "port": "portleft_${node.id}"
    }
  }
]     