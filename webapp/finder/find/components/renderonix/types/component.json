[
	{
        "type": "draw2d.shape.composite.Group",
        "id": "component${node.id}",
	    "x": ${node.offetX(220,0)},
	    "y": ${node.offetY(50,20)},
	    "width": 200,
	    "height": 60,
        "bgColor": "#60729e",
        "color": "#000000",
        "stroke": 1,
        "cssClass": "labelGroup",
        "radius": 4,
        "ports": [
        {
            "id": "port1${node.id}",
            "name": "labelPortLeft",
		    "type": "draw2d.HybridPort",
		    "bgColor": "#60729e",
		    "color": "#4d5d80",
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
		    "width": 16,
		    "height": 16,
            "locatorAttr": {
              "x": 0,
              "y": 50
            }
        },
        {
            "id": "portright${node.id}",
            "name": "labelPortRight",
            "type": "draw2d.HybridPort",
		    "bgColor": "#60729e",
		    "color": "#4d5d80",
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
		    "width": 16,
		    "height": 16,
            "locatorAttr": {
              "x": 100,
              "y": 50
            }
        }]
    },
    {
        "type": "draw2d.shape.basic.Label",
        "id": "componenttitle${node.id}",
	    "x": ${node.offetX(200,5)},
	    "y": ${node.offetY(50,20)},
        "text": "$node.getName()",
      ##  "textAnchor": "middle",
        "stroke": 0,
        "fontSize": 20,
        "fontColor": "#4d5d80",
        "bgColor": "none",
        "selectable": false,
        "draggable": false,
        "cssClass": "titleLabel",
        "composite": "component${node.id}"
    }
 ]     