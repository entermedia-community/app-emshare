[
  {
    "type": "draw2d.shape.composite.Group",
    "id": "id_${node.id}",
    "x": ${node.x},
    "y": ${node.y},
    "width": 50,
    "height": 40,
    "bgColor": "#ffffff",
    "color": "#4d5d80",
    "stroke": 1,
    "cssClass": "labelGroup",
    "radius": 4,
    "selectable": true,
    "draggable": false,
    "ports": [
      {
        "id": "portleft_${node.id}",
        "name": "portleft_${node.id}",
		    "type": "draw2d.HybridPort",
        "width": 4,
        "height": 4,
        "selectable": false,
        "draggable": false,
        "bgColor": "#60729e",
        "color": "#4caf50", 
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator", 
        "locatorAttr": {
          "x": 0,
          "y": 50
        }
      },
      {
        "id": "portright_${node.id}",
        "name": "portright_${node.id}",
        "type": "draw2d.HybridPort",
        "width": 4,
        "height": 4,
        "selectable": false,
        "draggable": false,
        "bgColor": "#60729e",
        "color": "#4caf50", 
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
        "locatorAttr": {
          "x": 100,
          "y": 50
        }
      }
    ] 
  },
  {
    "type": "draw2d.shape.basic.Label",
    "id": "connectorlabel${node.id}",
    "x": ${node.x},
    "y": ${node.y},
    "text": "$node.getTextTrim()",
    "stroke": 0,
    "fontSize": 18,
    "fontColor": "#4d5d80",
    "bgColor": "none",
    "selectable": false,
    "draggable": false,
    "cssClass": "titleLabel",
    "composite": "id_${node.id}"
  },
  {
    "type": "draw2d.Connection",  
    "cssClass": "draw2d_Connection", 
    "color": "#4caf50", 
    "outlineColor": "#f5f5f5", 
    "selectable": false,
    "draggable": false,
    "router": "draw2d.layout.connection.InteractiveManhattanConnectionRouter",
    "radius": 16,
    "source": {
      "node": "id_${node.sourceId}",
      "port": "portright_${node.sourceId}"
    },
    "target": {
      "node": "id_${node.id}",
      "port": "portleft_${node.id}"
    }
  }
]