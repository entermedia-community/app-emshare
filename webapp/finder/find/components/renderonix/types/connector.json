[
  {
    "type": "draw2d.shape.composite.Group",
     "id": "connector${node.id}",
	"x": ${node.offetX(220,0)},
	"y": ${node.offetY(60,10)},
    "width": 200,
    "height": 40,
    "bgColor": "#ffffff",
    "color": "#4d5d80",
    "stroke": 1,
    "cssClass": "labelGroup",
    "radius": 4,
    "ports": [
        {
            "id": "portleft${node.id}",
            "name": "labelPortLeft",
		    "type": "draw2d.HybridPort",
		    "bgColor": "#60729e",
		    "color": "#4d5d80",
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
		    "width": 16,
		    "height": 16,
            "locatorAttr": {
              "x": 0,
              "y": 50
            }
        },
        {
            "id": "portright${node.id}",
            "name": "labelPortRight",
            "type": "draw2d.HybridPort",
		    "bgColor": "#60729e",
		    "color": "#4d5d80",
		    "port": "draw2d.HybridPort",
		    "locator": "draw2d.layout.locator.XYRelPortLocator",
		    "width": 16,
		    "height": 16,
            "locatorAttr": {
              "x": 100,
              "y": 50
            }
        }] 
  },
  {
    "type": "draw2d.shape.basic.Label",
    "id": "connectorlabel${node.id}",
	"x": ${node.offetX(220,5)},
	"y": ${node.offetY(60,5)},
    "text": "$node.getName()",
   ## "textAnchor": "middle",
    "stroke": 0,
    "fontSize": 20,
    "fontColor": "#4d5d80",
    "bgColor": "none",
    "selectable": false,
    "draggable": false,
    "cssClass": "titleLabel",
    "composite": "connector${node.id}"
  }
]