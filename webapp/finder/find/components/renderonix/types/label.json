[
  {
    "type": "draw2d.shape.composite.Group",
    "id": "componentId",
    "x": "attr.x",
    "y": "attr.y",
    "width": 300,
    "height": 50,
    "bgColor": "#ffffff",
    "color": "#4d5d80",
    "stroke": 1,
    "cssClass": "labelGroup",
    "radius": 4
  },
  {
    "type": "draw2d.shape.basic.Label",
    "id": "componentId-title",
    "x": "attr.x + 300 / 2 - 5",
    "y": "attr.y + 15",
    "text": "attr.title",
    "textAnchor": "middle",
    "stroke": 0,
    "fontSize": 20,
    "fontColor": "#4d5d80",
    "bgColor": "none",
    "selectable": false,
    "draggable": false,
    "cssClass": "titleLabel",
    "composite": "componentId"
  }
]
